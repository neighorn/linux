bank:
	/usr/local/dirvish
	/media/disk/dirvish
exclude:
	/media/
	/mnt/
	/proc/
	/sys/
	/dev/shm
Runall:
	xxx

rsync-options:
log: gzip

image-default: %Y-%m-%d-%a
image-temp: latest

branch-default: daily
#expire-default: +95 days
expire-rule:
#       MIN HR    DOM   MON       DOW  STRFTIME_FMT
	*   *     1-7   *         0    +6 months
	*   *     15-21 *         1    +3 months
	*   *     *     *         *    +35 days
